generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model Location {
  id         Int    @id @default(autoincrement()) @map("id")
  name       String @map("name")
  code       String @unique @map("code")
  laboratory String @map("laboratory")

  @@map("locations")
}
